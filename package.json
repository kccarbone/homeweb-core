  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kccarbone/homeweb-core.git"
  },
  "scripts": {
    "local": "tsc -w",
    "build": "tsc && cp -R config build/config && cp package.json build/ && chmod +x build/tests/*.js",
